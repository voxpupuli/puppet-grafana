{
  "name": "puppet-grafana",
  "version": "8.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "This module provides Grafana, a dashboard and graph editor for Graphite and InfluxDB.",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-grafana.git",
  "project_page": "https://github.com/voxpupuli/puppet-grafana",
  "issue_url": "https://github.com/voxpupuli/puppet-grafana/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.20.0 < 8.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 1.0.1 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
